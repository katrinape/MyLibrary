<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mylibrary.controllers.CategoryController">
    <HBox alignment="BASELINE_CENTER" spacing="10.0">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <Label prefWidth="100.0" text="%add_category" />
        <TextField prefWidth="100.0" />
        <Button mnemonicParsing="false" prefWidth="100.0" text="%add_button" />
        <Label prefWidth="150.0" text="%choose_category" />
        <ComboBox prefWidth="150.0" />
    </HBox>
</VBox>
